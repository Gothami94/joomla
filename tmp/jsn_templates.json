        <style type="text/css">
            #myList li{ display:none;
                }
                #loadMore {
                    color:green;
                    cursor:pointer;
                }
                #loadMore:hover {
                    color:black;
                }
                #showLess {
                    color:red;
                    cursor:pointer;
                }
                #showLess:hover {
                    color:black;
            }
        </style>
        <script type="text/javascript">
            $(document).ready(function () {
                size_li = $("#myList li").size();
                x=12;
                $('#myList li:lt('+x+')').show();
                $('#loadMore').click(function () {
                    x= (x+12 <= size_li) ? x+12 : size_li;
                    $('#myList li:lt('+x+')').show();

                    if( size_li == x ) {
                        $( '#loadMore' ).hide();
                    }


                });
                $('#showLess').click(function () {
                    x=(x-1<0) ? 1 : x-1;
                    $('#myList li').not(':lt('+x+')').hide();
                });
            });
        </script>
